<div class="tr">
  <div class="td round border light background padding">
    <div class="fullwidth table">
      <div class="tr">
        <div class="td" style="width: 5em;">
          <img src="/img/thermbomo110.png" style="width: 5em; height: 4em;">
        </div>
        <div class="td padding center">{{display_name}}<br>★★★☆☆</div>
        <div class="td center" style="width: 3em;">
          <a href="{{url}}"><button class="dark background">Enter</button></a>
        </div>
      </div>
    </div>
    <hr>
    <div class="fullwidth table">
      <div class="tr">
        <div class="td">
          <div style="max-width: 25em; max-height: 5em; overflow: hidden;">
          {{description}}
          </div>
        </div>
        <div class="td center" style="min-width: 4em;">Last Updated:<br>{{last_modified}}</div>
      </div>
    </div>
    <div class="fullwidth table">
      <div class="tr">
        <div class="td center">
          <a href="#"><button type="button" class="half">Follow</button></a>
        </div>
      </div>
    </div>
  </div>
</div>
