<h2 class="text island">{{{video.name}}} | {{path.name}}</h2>
<div class="row">
  <div class="wide video column">
    <img class="ratio" src="/16x9.png"/>
    <iframe class="dark background border" src="{{{video.url}}}" frameborder="0" allowfullscreen></iframe>
  </div>
  <div class="thin column">
    <div class="dark background box">
      <h2 class="center text">{{path.name}}</h2>
      {{> pathresults}}
    </div>
  </div>
</div>
